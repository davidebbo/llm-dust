  "version": "0.2.0",
  "configurations": [
    {
      "name": "Install plugin",
      "type": "debugpy",
      "request": "launch",
      "module": "llm",
      "args": ["install", "-e", "."],
      "console": "integratedTerminal"
    },
    {
      "name": "List plugings",
      "type": "debugpy",
      "request": "launch",
      "module": "llm",
      "args": ["plugins"],
      "console": "integratedTerminal"
    },
    {
      "name": "List models",
      "type": "debugpy",
      "request": "launch",
      "module": "llm",
      "args": ["models"],
      "console": "integratedTerminal"
    },
    {
      "name": "Run model",
      "type": "debugpy",
      "request": "launch",
      "module": "llm",
      "args": ["-m", "fruity", "What's the capital of France?"],
      "console": "integratedTerminal"
    }
  ]
}
